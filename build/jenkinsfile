pipeline {
  agent {
    label 'agent1'
   }

  stages{
        stage("Clean Workspace") { 
            steps {
                cleanWs()
            }
        }

        stage("App Codeckeckout"){
            steps {
                git branch: "main", url:"https://github.com/prateekkumawat/jenkins-11-project-ci.git"
            }  
        }

        stage("build Image"){
            steps{
                sh 'docker build -t jenkinsimage:v1 .'
            }
        }
   }
}